<script>
    function photoUpload(input) {
        const file = $(input).get(0).files;
        let ext_allowed_array = ["image\/jpeg","image\/png","image\/gif"];

        if (file.length > 0) {
            if(file[0].size > 2097152) {
                Swal.fire({
                    type: 'error',
                    title: 'Errore',
                    text: 'Il file è più grande della dimensione consentita (Dimensione Massima: 2 MB)',
                });
                return false;
            }
            if(!ext_allowed_array.includes(file[0].type)) {
                Swal.fire({
                    type: 'error',
                    title: 'Errore',
                    text: "Formato non valido",
                });
                return false;
            }

            const reader = new FileReader();
            reader.onload = function(e) {
                $('#img-preview-profile-picture').attr('style', 'background-image: url("' + e.target.result + '")');
                $('#border-img-preview-profile-picture #trash-remove-preview').show();
                $('#photo-field-preview').show();
                $('#photo-field-input').hide();
            };
            reader.readAsDataURL(file[0]);
        } else {
            $('#border-img-preview-profile-picture #trash-remove-preview').hide();
            $('#photo-field-preview').hide();
            $('#photo-field-input').show();
        }
    }
</script>

