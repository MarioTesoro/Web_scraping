        <script>

            /* CLICK PER BOTTONI DI ORDINAMENTO DEGLI ANNUNCI */
            $('body').on('click', '#order li > a', function (event) {
                event.preventDefault();
                researchAnnounces(1, $(this).attr('data-order'));
            });

            /* FUNZIONE PER RICHIAMARE IL FRAME SUGLI ANNUNCI E RICARICARLO */
            function researchAnnounces(pageNumber, order, mobile = false) {
                const $tabAnnunci = $('#tab-annunci');
                const $annSpinner = $('#annunci-spinner');
                const oldView = $tabAnnunci.html();

                const url = $('#url-for-announces').val();
                const mobileString = mobile ? 'Mobile' : '';

                /* Informazioni di ricerca */
                const regionFilter = $('#regionFilter' + mobileString).val();
                const functionFilter = $('#functionFilter' + mobileString).val();
                const projectFilter = $('#projectFilter' + mobileString).val();
                const textFilter = $('#textFilter' + mobileString).val();
                const divisionFilter = $('#divisionFilter' + mobileString).val();
                const companyFilter = $('#companyFilter' + mobileString).val();

                /* Inserisco la gif di caricamento */
                $tabAnnunci.slideUp(200);
                setTimeout(function () {
                    $tabAnnunci.html('');
                    $annSpinner.show();

                    $.ajax({
                        url: url,
                        type: "POST",
                        data: {
                            'act1': 'vacancyListCareer',
                            'section': 'AW2qlWnc5mnmZaELGY1W',
                            'order': order,
                            'page': pageNumber,
                            'region': regionFilter,
                            'function': functionFilter,
                            'project': projectFilter,
                            'text': textFilter,
                            'division': divisionFilter,
                            'company': companyFilter
                        },
                        success: function (response) {
                            response = $.parseJSON(response);
                            if (response.success === true) {
                                $tabAnnunci.html(response.data);
                                $annSpinner.hide();
                                $tabAnnunci.slideDown(200);
                            } else {
                                alert('Error');
                                $annSpinner.hide();
                                $tabAnnunci.html(oldView);
                                $tabAnnunci.slideDown(200);
                            }
                        },
                        /* reimposto la lista che c'era prima */
                        error: function () {
                            alert('Error');
                            $annSpinner.hide();
                            $tabAnnunci.html(oldView);
                            $tabAnnunci.slideDown(200);
                        }
                    });
                }, 200);
            }
        </script>
