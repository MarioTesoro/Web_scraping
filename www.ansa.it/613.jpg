            $(field_input).find('.trash-remove-file').show();
        } else {
            $(field_input).find('.text-note-input-file').html('Clicca qui (o fai drag and drop) per <strong>Caricare un file</strong><br><small style="font-size: 0.9em">pdf, doc, docx, xls, xlsx, ppt, pptx, odt, odp, ods, txt, rtf, jpg, jpeg, gif, png (Max: 2 MB)</small>');
            $(field_input).find('.trash-remove-file').hide();
        }
    }
