    <style>
        .viaggiart-search {

            position:absolute;
            right: 0px;
            bottom: 20px;

        }

        .viaggiart-search
        .search-button {
           /* background: url(/viaggiart/it/assets/img/search.png) no-repeat center;
            text-indent: -9999px;
            overflow: hidden;*/
            color:white;
            cursor: pointer;
            background-color: #ea1f26;
            width: 50px
        }

        .viaggiart-search
        .form-control {
            border-color: #ea1f26;
        }

        #header-va {
            width: 100%;
            height: auto;
        }

        #top-va-images {
            width: 100%;
            height: auto;
            margin-bottom: 20px;
        }

        hr.va {
            display: block;
            margin-top: 20px;
            margin-bottom: 20px;
            border: 0;
            border-top: 1px solid #ea1f26;
        }

        @media (min-width: 992px){
            .top{
                margin-top: 20px;
                margin-bottom: 20px;
            }

            .big-image {
                min-height: 350px;
            }

            .evento {
                height: 65px;
            }

            .evento .title {
                font-size: 12px;
                font-weight: bold;
                position: relative;
            }

            .evento .subtitle {
                font-size: 10px;
                position: relative;
            }

            .evento .city-name {
                font-size: 10px;
                position: relative;
            }

            .evento .small {
                display: block;
            }

            .evento .large {
                display: none;
            }

            .cella-big{
                margin-bottom: 40px;
            }
        }


        @media (max-width: 991px){
            .big-image {  height: 180px;  }

            .top{
                margin-top: 20px;
                margin-bottom: 20px;
            }

            .evento .title {

            }

            .evento .subtitle {
                font-size: 12px;
                top: -4px;
                position: relative;
            }

            .evento .city-name {
                font-size: 12px;
                top: -4px;
                position: relative;
            }

            div.title.small {
                display: none !important;
            }

            .evento .large {
                display: block;
            }
            .cella-small {
                margin-top: 10px;
            }
            .cella-big{
                margin-bottom: 40px;
            }


        }

        .big-image {
            height: 180px;
            overflow: hidden;
            width: 100%;
            background-image: url("/viaggiart/it/assets/regioni/calabria/san-nicola-arcella.jpg");
            background-position: center bottom;
            background-repeat: no-repeat;
            position: relative;
            background-size: cover;
        }

        .square-image {
            min-height: 350px;
            overflow: hidden;
            position: relative;
            background-position: center bottom;
            background-repeat: no-repeat;
            background-size: cover;
            margin-bottom: 20px;
        }

        .square-image-small {
            width: 100%;
            height: auto;
            position: relative;
        }

        .square-image-very-small {
            width: 100%;
            height:100%;
            /*margin-top: 10px;*/
            position: relative;

        }



        .place, .city {
            width: 100%;
            height: 100%;
            float: left;
            overflow: hidden;
            position: relative;
            text-align: center;
            cursor: default;
        }


        .big-image .photo { position: absolute; top: 0px; right: 0px; width: 100%; padding: 4px 10px; text-align: right; background-color: rgba(0,0,0,0.20); }
        .big-image .photo a { color: whitesmoke; font-size: 12px; }
        .big-image .photo a:hover { text-decoration: none; }

        .city .overlay { position: absolute; bottom: 0px; padding: 10px; left: 0px; width: 100%; color:white; background-color: #D01E25;;}
        .place .overlay { position: absolute; bottom: 0px; padding: 10px; left: 0px; width: 100%; color:white; background-color: rgba(0,0,0,0.40);}

        .city .overlay a.info { color: #fff; font-size: 18px;}
        .place .overlay a.info { color: #fff; font-size: 12px;}

        .suggerimenti {
            margin-top: 30px;
            font-size: 20px;
        }

        @media (min-width: 992px){
            .col-md-6 {
                width: 480px;
            }
        }

