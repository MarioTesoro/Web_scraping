	$("a[name=add_facet_filter]").click(function (event) {
		event.preventDefault();
		var facet = $(this).attr('id');
		var value = $(this).html();
		var filterName = facet.replace("facet_","");
		var idx = filterName.lastIndexOf("_");
		filterName = filterName.substring(0, idx);
		var filter = filterName + '@' + value;
		$('#searchform2').append('<input type="hidden" name="fl" value="' + filter + '" />');
		$("#start").val(0);
		$("#searchform2").submit();
	});
	
